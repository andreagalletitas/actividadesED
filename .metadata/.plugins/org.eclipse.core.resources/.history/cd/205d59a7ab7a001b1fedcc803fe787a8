package pojo;

public class Coche {
	private int id;
	private String matricula;
	private String modelo;
	private String marca;
	private double km;
	
	public Coche (int vId, String vMat, String vMod, String vMarca, double vKm) {
		id = vId;
		matricula = vMat;
		modelo = vMod;
		marca = vMarca;
		km = vKm;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getMatricula() {
		return matricula;
	}

	public void setMatricula(String matricula) {
		this.matricula = matricula;
	}

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public double getKm() {
		return km;
	}

	public void setKm(double km) {
		this.km = km;
	}
	
	
}
